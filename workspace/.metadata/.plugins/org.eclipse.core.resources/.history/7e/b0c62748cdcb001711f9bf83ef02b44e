import java.util.HashMap;
import java.util.Map;

public class TC {

	public static void main(String a[]) {
		Map propertyMap = new HashMap();
		propertyMap.put("createTable", "TRUE");
		String createTable = propertyMap.get("createTable")  == null ? "true" : propertyMap.get("createTable").toString();
		
		 if(Boolean.valueOf(createTable.toLowerCase())) {
			 System.out.println("true");
		 } else {
			 System.out.println("false`");
			 
		 }
		 
	}
	
}
